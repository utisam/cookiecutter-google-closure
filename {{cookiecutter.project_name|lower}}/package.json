{
  "name": "{{cookiecutter.project_name}}",
  "version": "0.0.0",
  "description": "{{cookiecutter.description}}",
  "private": true,
  "dependencies": {
    "google-closure-compiler": "~20160619.0.0",
    "google-closure-templates": "~20150410.0.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "rules": {
      "comma-dangle": [2, "always-multiline"],
      "block-scoped-var": 2,
      "eqeqeq": [2, "smart"],
      "no-eq-null": 2,
      "no-implicit-coercion": 2,
      "no-multi-spaces": 2,
      "no-console": 0,
      "no-unused-vars": ["error", { "vars": "local" }],
      "indent": [2, 2],
      "semi": [2, "always"],
      "complexity": 1
    },
    "extends": "eslint:recommended"
  }
}
